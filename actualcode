while wait() do
    if convert == true then
        game:GetService("ReplicatedStorage").ExchangeEvent:FireServer()
        end
    if autoc == true then
            if not plr.Backpack:FindFirstChild("Card") and not plr.Character:FindFirstChild("Card") then
    if rand == false then
    local args = {
