         if plr.attributes.Token.Value > 0 then
        local d = workspace:GetChildren()
        for i=1, #d do
            if (d[i].Name == "Machine") then
                local mach = d[i]
                print("omgmach")
                if mach.Owner.Value == nil then
                    realmach = mach
                    if plr.attributes.Token.Value > 0 then
                    repeat
                    fireclickdetector(realmach.ClickDetector)
                    wait(5)
                    until plr.attributes.Token.Value == 0
                    break
                    end
                    end
            end
        end
        else
             DiscordLib:Notification("Notification", "You have no tokens", "ok")
            end
