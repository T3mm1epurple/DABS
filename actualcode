print(bool)
end)

drops:Button("Add option", function()
local d = workspace:GetDescendants()
for i=1, #d do
if (d[i]:FindFirstChild("Price") then
local item = d[i]
drop:Add(d[i].Price.TextLabel.Text.." - "..d[i].Name)
end
end
end)

