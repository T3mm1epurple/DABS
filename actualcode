end)

bing:Button("Get bingo card", function()
if plr.Backpack:FindFirstChild("BingoCard") or plr.Character:FindFirstChild("BingoCard") then
    DiscordLib:Notification("Notification", "You already have a card", "damn")
    else
DiscordLib:Notification("Notification", "Got card", "Okay")
