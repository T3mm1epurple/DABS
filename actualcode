        local char = plr.Character
        local d = workspace:GetChildren()
        for i=1, #d do
            if (d[i].Name == "VomitPart") then
                if autom == true then
                vomit = d[i]
                char.HumanoidRootPart.CFrame = CFrame.new(vomit.Position + Vector3.new(0, 5, 0))
                wait(1)
                if vomit:IsDescendantOf(game) then
                fireproximityprompt(vomit.ProximityPrompt)
                wait(3)
                end
                end
                end
        end
        end
    if autod == true then
        print("test")
    local d = plr.Backpack:GetChildren()
