        if (d[i]:FindFirstChild("OOF")) then
            tool = d[i]
            print("hi")
        end
    end
    print("test2")
    local s = plr.Character:GetChildren()
    for f=1, #s do
        if (s[f].ClassName == "Tool") then
            if (s[f]:FindFirstChild("OOF")) then
                tool = s[f]
                print("hi")
                end
        end
    end
    print("WHY")
    if tool:IsDescendantOf(game) then
        local args = {
    [1] = 1
}

tool.OOF:FireServer(unpack(args))
tool.Coward:FireServer()
tool.Parent = plr.Character
if safed == false then
wait(.1)
else
    wait(1)
end
end
